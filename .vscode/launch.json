  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Logs",
      "program": "${workspaceFolder}/logs.js",
      "skipFiles": ["<node_internals>/**"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Server",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/index.js"
    }
  ],
  "compounds": [
    {
      "name": "Server and Logs",
      "configurations": ["Launch Server", "Launch Logs"]
    }
  ]
}
